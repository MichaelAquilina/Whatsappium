{
  "applications": {
   "gecko": {
     "strict_min_version": "42.0"
   }
 },

  "manifest_version": 2,
  "name": "Whatsappium",
  "version": "0.3.1",

  "description": "Adds keyboard shortcuts to Whatsapp Web",

  "content_scripts": [
    {
      "matches": ["*://web.whatsapp.com/*"],
      "js": ["whatsappium.js"]
    }
  ]

}
